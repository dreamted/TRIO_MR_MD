     89          /*******************************************************************************
     90          * Function Name  : RTC_GetCounter
     91          * Description    : Gets the RTC counter value.
     92          * Input          : None
     93          * Output         : None
     94          * Return         : RTC counter value.
     95          *******************************************************************************/
     96          u32 RTC_GetCounter(void)
     97          {
     98            u16 tmp = 0;
     99            tmp = RTC->CNTL;
    100          
    101            return (((u32)RTC->CNTH << 16 ) | tmp) ;
    102          }
